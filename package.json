{
  "name": "@jtarchie/some-router",
  "version": "0.0.4",
  "description": "router for http requests that is platform agnostic",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.11.22",
    "@swc/jest": "^0.2.38",
    "@types/benchmark": "^2.1.5",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "benny": "^3.7.1",
    "express": "^5.1.0",
    "find-my-way": "^9.3.0",
    "regenerator-runtime": "^0.14.1",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "scripts": {
    "bench": "ts-node src/benchmark.ts",
    "build": "npx tsc",
    "fmt": "deno fmt src/ *.md *.json",
    "lint": "deno lint --rules-exclude=ban-types src/",
    "prepublishOnly": "yarn build",
    "test": "yarn run lint && yarn run fmt && npx vitest"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "target": "es5"
          },
          "sourceMaps": true
        }
      ]
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
