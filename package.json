{
  "name": "some-router",
  "version": "0.0.1",
  "description": "router for http requests that is platform agnostic",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.2.128",
    "@swc/jest": "^0.2.15",
    "@types/benchmark": "^2.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "benny": "^3.7.1",
    "express": "^4.17.2",
    "find-my-way": "^5.1.1",
    "jest": "^27.4.7",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "bench": "ts-node src/benchmark.ts",
    "build": "npx tsc",
    "fmt": "deno fmt src/ *.md",
    "lint": "deno lint --rules-exclude=ban-types src/",
    "test": "yarn run lint && yarn run fmt && npx jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "target": "es5"
          },
          "sourceMaps": true
        }
      ]
    }
  }
}
