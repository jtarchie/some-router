{
  "name": "@jtarchie/some-router",
  "version": "0.0.3",
  "description": "router for http requests that is platform agnostic",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.2.244",
    "@swc/jest": "^0.2.22",
    "@types/benchmark": "^2.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "benny": "^3.7.1",
    "express": "^4.18.1",
    "find-my-way": "^7.0.1",
    "jest": "^29.0.1",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "bench": "ts-node src/benchmark.ts",
    "build": "npx tsc",
    "fmt": "deno fmt src/ *.md *.json",
    "lint": "deno lint --rules-exclude=ban-types src/",
    "prepublishOnly": "yarn build",
    "test": "yarn run lint && yarn run fmt && npx jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "target": "es5"
          },
          "sourceMaps": true
        }
      ]
    }
  }
}
